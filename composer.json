{
  "name": "takatost/php-pubsub-cmq",
  "description": "A Tencent CMQ adapter for the php-pubsub package.",
  "license": "MIT",
  "authors": [
    {
      "name": "takatost",
      "email": "takatost@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/support": ">=5.2",
    "superbalist/php-pubsub": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Takatost\\PubSub\\CMQ\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5"
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.dev.vstore.vchangyi.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    {
      "packagist": false
    }
  ]
}